{
  "parser":"babel-eslint",
  "rules": {
    "strict":2,
    "no-extra-semi":2,
    "accessor-pairs":2,
    "brace-style":2,
    "comma-dangle":2,
    "consistent-return":2,
    "dot-location":2,
    "dot-notation":2,
    "eol-last":2,
    "eqeqeq":2,
    "indent":2,
    "keyword-spacing":2,
    "no-bitwise":2,
    "no-inner-declarations":2,
    "no-multi-spaces":2,
    "no-restricted-syntax":2,
    "no-shadow":2,
    "no-unused-expressions":2,
    "no-unused-vars":2,
    "no-useless-concat":2,
    "quotes":2,
    "space-before-blocks":2,
    "space-before-function-paren":0
  }
}